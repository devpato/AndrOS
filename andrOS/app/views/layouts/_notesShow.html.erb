<div class="draggable" style="display:show;" id="notesFrame" data-app="notes">
    <div class="window" id="notes">
        <div class="titleBar">
            <span class="close" onclick="closeApp('notes')"></span>
            <span class="max" onclick="snapApp('notes', 2)"></span>
            <span class="rest none" onclick="snapApp('notes', 0)"></span>
            <span class="min" onclick="minApp('notes')"></span>
            <span class="titleText">Notes</span>

            <span class="rightArrow" onclick="snapApp('notes', 1)"></span>
            <span class="leftArrow" onclick="snapApp('notes', -1)"></span>
        </div>
        <div class="windowContent">
            <%= render 'apps/notes' %>
        </div>
    </div>
</div>