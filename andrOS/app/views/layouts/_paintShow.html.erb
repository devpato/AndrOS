<div class="draggable" style="display:show;" id="paintFrame" data-app="paint">
    <div class="window" id="paint">
        <div class="titleBar">
            <span class="close" onclick="closeApp('paint')"></span>
            <span class="max" onclick="snapApp('paint', 2)"></span>
            <span class="rest none" onclick="snapApp('paint', 0)"></span>
            <span class="min" onclick="minApp('paint')"></span>
            <span class="titleText">Paint</span>

            <span class="rightArrow" onclick="snapApp('paint', 1)"></span>
            <span class="leftArrow" onclick="snapApp('paint', -1)"></span>
        </div>
        <div class="windowContent">
            <%= render 'apps/paint' %>
        </div>
    </div>
</div>
