            <ul class="nav navbar-nav">
                <li class="toggleApps"><a href="#" id="toggleApps"><span class="glyphicon glyphicon-th"></span></a></li>
            </ul>
            
            
            
            <ul class="nav navbar-nav navbar-right">
                <% if current_user %>
                    <li><a href="#" data-toggle="modal" data-target="#myModal" data-backdrop="static"><%= current_user.name %></a></li>
                
                    <!-- Modal -->
                    <div id="myModal" class="modal fade" role="dialog">
                        <div class="modal-dialog">

                            <!-- Modal content-->
                            <div class="modal-content">
                                <div class="modal-header">
                                    <!--button type="button" class="close" data-dismiss="modal">&times;</button-->
                                    <h4 class="modal-title"><%= current_user.name %></h4>
                                </div>
                                <div class="modal-body">
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-lg-3">
                                                <%= image_tag current_user.image_url, alt: current_user.name, :class => "img-rounded"%>
                                            </div>
                                            <div class="col-lg-9">
                                                <ul>
                                                    <li><strong>Provider:</strong> <%= current_user.provider %></li>
                                                    <li><strong>Member Since:</strong> <%= current_user.created_at.strftime("%B %d, %Y") %></li>
                                                    <li><strong>Last Login:</strong> <%= current_user.updated_at.strftime("%B %d, %Y") %></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <div class="btn-group btn-group-sm btn-group-dark">
                                        <%= link_to "Log Out", logout_path, :class => "btn btn-default btn-dark", method: :delete, :role => "button" %>
                                        <button type="button" class="btn btn-default btn-dark" data-dismiss="modal">Close</button>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                
                <% else %>
                    <li><%= link_to 'Log In', '/auth/google' %></li>
                <% end %>
                <li><a href="#"><span id="clock"></span></a></li>
            </ul>
            
            

            
            
            
