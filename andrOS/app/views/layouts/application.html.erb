<html>
<head>

    <title>andrOS</title>
    
    
    
    <%= stylesheet_link_tag    'application', media: 'all'%>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
    
</head>
<body onload="startTime()">
    
    <nav class="navbar navbar-inverse">
        <div class="container-fluid">
            <!--div class="navbar-header">
