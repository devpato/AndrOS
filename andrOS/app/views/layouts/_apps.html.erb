<script>
  $(document).ready(function () {

    (function ($) {

        $('#filter').keyup(function () {

            var rex = new RegExp($(this).val(), 'i');
            $('.searchable td').hide();
            $('.searchable td').filter(function () {
                return rex.test($(this).text());
            }).show();
        })
    }($));

});
</script>